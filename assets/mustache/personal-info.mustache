<section class="section-form-login">
	<fieldset>
		<div class="row"><div class="twelve columns"><label>Personal Information</label><input type="text" name="FirstName" placeholder="First Name"/></div></div>
		<div class="row"><div class="twelve columns"><label></label><input type="text" name="LastName" placeholder="Last Name"/></div></div>
	</fieldset>
	<fieldset>
		<div class="row"><div class="twelve columns"><label>Your Login</label><input type="text" name="Email" placeholder="Email"/></div></div>
		<div class="row"><div class="twelve columns"><label></label><input id="password" type="password" name="Password" placeholder="Password"/></div></div>
		<div class="row"><div class="twelve columns"><label></label><input name="VerifyPassword" type="password" placeholder="Confirm Password"/></div></div>
		<div class="row"><div class="twelve columns"><label></label>
			<select>
				<option>Country of Residence</option>
			</select>
		</div></div>
		<div class="row"><div class="twelve columns"><label></label>
			<select>
				<option>State</option>
			</select>		
		</div></div>
		<div id="ErrorDiv" class="row">
			<div class="twelve columns">
			<div id="ErrorDivMsg" class="hide">All fields are required</div>
			</div>
		</div>
		<div class="row"><div class="six columns">
		<input id="Submit" class="button" type="submit" value="Register"/>
		</div><div class="six columns"><a href="">Cancel</a></div></div>
	</fieldset>
</section><!--/.section-content-->