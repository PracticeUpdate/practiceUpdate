
{{#user-group}}
<section class="user-group">
<h2>{{user-group-title}}</h2>
	{{#users}}

		{{#user}}
		<div class="profile-card user">
		<!-- TODO: add microdata to markup -->
			{{#photo}}
			<div class="profile-photo">
				<div class="user-photo">
					<img class="photo" src="{{{url}}}"/>
				</div><!-- /.user-photo -->
			</div><!-- /.profile-photo -->
			{{/photo}}
			<div class="profile-overview">
				{{#name}}
				<div class="user-name">
				<!-- {{firstName}} {{middleName}} {{lastName}} {{honorific}} -->
				<!-- NOTE: honorific-prefix is unnecesary when honorific-suffix is displayed  -->
					<span class="given-name">{{firstName}}</span>
					<abbr class="additional-name">{{middleName}}</abbr>
					<span class="family-name">{{lastName}}</span>
					<span class="honorific-suffix">{{honorific}}</span>
				</div><!-- /.user-name -->
				{{/name}}
				<dl class="user-detail-list">
					<dt>Profession:</dt>
					<dd>{{profession}}</dd>
					<dt>Member Since:</dt>
					<dd><date>{{date-joined}}</date></dd>
					</dd>
					</dl><!--/.user-detail-list -->
				</div><!--/.profile-overview -->
			</div><!-- /.profile-card -->
		{{/user}}
	{{/users}}
	</section>
{{/user-group}}


<div itemscope itemtype="http://data-vocabulary.org/Person" class="{{source.type}}-name">  
 <img src="{{{img.url}}}" itemprop="photo" class="photo">  
  <span itemprop="name">{{firstName}} {{middleName}} {{lastName}} {{honorific}}</span>
  <dl><span itemprop="title">writer</span>  
  <a href="http://net.tutsplus.com" itemprop="affliation">Nettuts+</a>.  
  I live in  
  <span itemprop="address" itemscope  
    itemtype="http://data-vocabulary.org/Address">  
    <span itemprop="locality">Memphis</span>,  
    <span itemprop="region">TN</span>  
  </span>
  Member since:
  </dl>
  <div class="user-bio"></div>
</div> 