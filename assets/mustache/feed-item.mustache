{{#items}}
<div class="stream-item-container">
<article class="stream-item {{type}} {{section}} {{#story-of-the-week}}story-of-the-week{{/story-of-the-week}} row-fluid">
					{{#source}}
					<div class="source-container">
						<div class="{{source.type}}-photo">
							<img class="photo" src="{{{photo.url}}}"/>
						</div>
					</div><!--/.source-container-->
					{{/source}}
					<div class="item-container">
						<header class="item-header">
							<hgroup class="pub-meta">
								<p><span class="item-type"><a href="#">{{section}}</a></span> <span class="item-pub-date">{{#pub-date}}{{date.month}} {{date.day}}, {{date.year}}{{/pub-date}}</span></p>
								<h3 class="item-title"><a href="{{url}}">{{title}}</a></h3>
							</hgroup><!--/.pub-meta-->
						</header>
						{{#item-content}}
						<div class="item-content-container">
							<div class="item-content">
							<div class="take-home-message"><p>{{{take-home-message}}}</p></div><!--/.take-home-message-->
							<div class="item-teaser"><p>{{{item-teaser}}}</p></div><!--/.item-teaser-->

							</div><!--/.item-content-->
						</div><!--/.item-content-container-->
						{{/item-content}}


					</div><!--/.item-container-->
						<footer class="item-footer">
						<div class="portal-meta">
							<p>Practice Relevance: {{practice-relevance}}</p>
						</div>
							<div class="item-actions">
								<div class="button-bar">
										<div href="#" class="button small" title="like this item"><span class="glyph general">c</span></div>
										<a href="#" ><i class="icon-share-alt"></i></a>
										<div href="#" class="button small dropdown">
											<a href="#" ><i class="icon-share-alt"></i></a>
											<ul>
												<li><a href="#">Send to twitter</a></li>
												<li><a href="#">Send to facebook</a></li>
												<li><a href="#">email link</a></li>
												<li><a href="#">Read Later</a></li>
											</ul>
										</div>
								</div><!--/.btn-toolbar-->
			        </div><!--/.item-actions-->
						</footer>
				</article></div><!--/.stream-item-->
			</div><!--/.stream-item-container-->
{{/items}}
<ul class="pagination">
  <li class="arrow unavailable"><a href="">&laquo;</a></li>
  <li class="current"><a href="">1</a></li>
  <li><a href="">2</a></li>
  <li><a href="">3</a></li>
  <li><a href="">4</a></li>
  <li class="unavailable"><a href="">&hellip;</a></li>
  <li><a href="">12</a></li>
  <li><a href="">13</a></li>
  <li class="arrow"><a href="">&raquo;</a></li>
</ul>